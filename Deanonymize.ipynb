   "source": [
    "import pandas as pd\n",
    "import json\n",
    "import hashlib\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "id": "3c0dbd54-5761-408c-8f50-14b9df09e947",
   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.read_csv(\"trip_data.csv\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "4e3ac24d-b6f4-468f-8d34-b1ddf491e8eb",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Función para convertir un número a una base con dígitos alfabéticos\n",
    "def baseN(num, b, numerals=\"ABCDEFGHIJKLMNOPQRSTUVWXYZ\"):\n",
    "    return ((num == 0) and numerals[0]) or (baseN(num // b, b, numerals).lstrip(numerals[0]) + numerals[num % b])\n",
    "\n",
    "cksum_number_map = {}\n",
    "\n",
    "# Función para agregar hashes a la tabla\n",
    "def add(s):\n",
    "    s_bytes = s.encode()  # Encode the Unicode string into bytes\n",
    "    cksum_number_map[hashlib.md5(s_bytes).hexdigest().upper()] = s\n",
    "\n",
    "\n",
    "# Generar hashes para los medallions\n",
    "for i in range(1000):\n",
    "    for j in range(26**3):\n",
    "        istr = str(i)\n",
    "        alpha_string = baseN(j, 26)\n",
    "        if len(alpha_string) == 1:\n",
    "            add(istr[0] + alpha_string + istr[1:])\n",
    "        else:\n",
    "            add(alpha_string + istr)\n",
    "\n",
    "# Generar hashes para los hack_licenses\n",
    "for i in range(int(6e6)):\n",
    "    cksum_number_map[hashlib.md5(('%0.6d' % i).encode()).hexdigest().upper()] = str(i)\n",
    "\n",
    "for i in range(5000000, 5900000):\n",
    "    cksum_number_map[hashlib.md5(('%0.7d' % i).encode()).hexdigest().upper()] = str(i)\n",
    "\n",
    "\n",
    "# Escribir la tabla arcoíris en un archivo JSON\n",
    "with open('rainbow.json', 'w') as f:\n",
    "    json.dump(cksum_number_map, f)\n",
    "    \n",
    "    \n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "14d7275f",
   "metadata": {},
   "outputs": [],
   "source": [
    "with open('rainbow.json', 'r') as f:\n",
    "    # Leer los contenidos del archivo\n",
    "    data = f.read()\n",
    "hashes = json.loads(data)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "id": "37244609",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Función para buscar un hash en la tabla arcoíris\n",
    "def lookup(s):\n",
    "    result = hashes.get(s.upper())\n",
    "    return result if result is not None else \"none\"\n",
    "\n",
    "# Apply anonymization to 'medallion' and 'hack_license' columns\n",
    "dfinal['medallion'] = df['medallion'].apply(lookup)\n",
    "dfinal['hack_license'] = df['hack_license'].apply(lookup)\n",
    "\n",
    "# Write the DataFrame to a CSV file with custom float formatting\n",
    "dfinal.to_csv(\"trip_data_deanonimized.csv\", index=False, float_format='%g')\n"
   ]
  },
